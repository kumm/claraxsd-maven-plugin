            <xs:element ref="vaadin-accordion"/>
            <xs:element ref="vaadin-audio"/>
            <xs:element ref="vaadin-browser-frame"/>
            <xs:element ref="vaadin-button"/>
            <xs:element ref="vaadin-calendar"/>
            <xs:element ref="vaadin-check-box"/>
            <xs:element ref="vaadin-color-picker"/>
            <xs:element ref="vaadin-color-picker-area"/>
            <xs:element ref="vaadin-combo-box"/>
            <xs:element ref="vaadin-css-layout"/>
            <xs:element ref="vaadin-custom-component"/>
            <xs:element ref="vaadin-custom-layout"/>
            <xs:element ref="vaadin-date-field"/>
            <xs:element ref="vaadin-drag-and-drop-wrapper"/>
            <xs:element ref="vaadin-embedded"/>
            <xs:element ref="vaadin-flash"/>
            <xs:element ref="vaadin-form"/>
            <xs:element ref="vaadin-form-layout"/>
            <xs:element ref="vaadin-grid"/>
            <xs:element ref="vaadin-grid-layout"/>
            <xs:element ref="vaadin-horizontal-layout"/>
            <xs:element ref="vaadin-horizontal-split-panel"/>
            <xs:element ref="vaadin-image"/>
            <xs:element ref="vaadin-inline-date-field"/>
            <xs:element ref="vaadin-label"/>
            <xs:element ref="vaadin-link"/>
            <xs:element ref="vaadin-list-select"/>
            <xs:element ref="vaadin-login-form"/>
            <xs:element ref="vaadin-menu-bar"/>
            <xs:element ref="vaadin-native-button"/>
            <xs:element ref="vaadin-native-select"/>
            <xs:element ref="vaadin-option-group"/>
            <xs:element ref="vaadin-panel"/>
            <xs:element ref="vaadin-password-field"/>
            <xs:element ref="vaadin-popup-date-field"/>
            <xs:element ref="vaadin-popup-view"/>
            <xs:element ref="vaadin-progress-bar"/>
            <xs:element ref="vaadin-progress-indicator"/>
            <xs:element ref="vaadin-rich-text-area"/>
            <xs:element ref="vaadin-select"/>
            <xs:element ref="vaadin-slider"/>
            <xs:element ref="vaadin-tab-sheet"/>
            <xs:element ref="vaadin-table"/>
            <xs:element ref="vaadin-text-area"/>
            <xs:element ref="vaadin-text-field"/>
            <xs:element ref="vaadin-tree"/>
            <xs:element ref="vaadin-tree-table"/>
            <xs:element ref="vaadin-twin-col-select"/>
            <xs:element ref="vaadin-upload"/>
            <xs:element ref="vaadin-vertical-layout"/>
            <xs:element ref="vaadin-vertical-split-panel"/>
